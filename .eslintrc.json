{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-shadow": "off",
        "no-param-reassign": "off",
        "eol-last": "off"
    },
    "ignorePatterns": [
        "dist/",
        "build/"
    ]
}
